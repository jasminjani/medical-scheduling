<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css"
    integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="/css/doctorPanel/viewfeedback.css">
  <link rel="stylesheet" href="/css/doctorPanel/a7dashboard.css">
  <link rel="stylesheet" href="/css/sideBar/newSidebar.css">
  <link rel="stylesheet" href="/css/adminPanel/allPatientDetails.css">
  <!-- <script src="/javascript/adminPanel/allPatientDetail.js"></script> -->
  <!-- <link rel="stylesheet" href="/css/doctorPanel/viewfeedback.css">
  <link rel="stylesheet" href="/css/doctorPanel/dashboard.css"> -->
  <title>View Payment</title>
</head>

<body onload="fetchPatients()">

  <div class="a5-container">

    <%- include('../../common/newSidebar/doctorSidebar') %>

      <div class="a5-section">

        <div class="a5-search">
          <div class="a5-searchbox">
            <img src="/assets/adminPanel/icons/search.svg" alt="">
            <input type="text" name="searchUser" id="a5-searchPatient" placeholder="Search Patient name" tabindex="1">
          </div>
          <p id="a5-btn-search" onclick="searchPatient()" tabindex="2">Search</p>
        </div>

        <div class="a5-all-user">

          <h2>View Payment History</h2>

          <div class="a5-user-table">

            <table id="a5-user-table">
              <tbody id="a5-tbody">

              </tbody>
            </table>

          </div>

        </div>

      </div>
      <table>

        <tr class="a7-feedback-table-header">
         
          <td><span>Name</span> </td>
          <td><span>Payment Amount</span></td>
          <td><span>Slot Time</span></td>
          <td><span>Slot Date</span></td>
          <td><span>Payment Date</span></td>
        </tr>
        <!-- fetch data using AJAX  -->
        <tr class="a7-feedback-table-data" id="rowdata"></tr>
        
      
      </table>
    </div></div>
    </div>
      
    <script>
          const fetchPaymentData = async() =>{
              const fetchdata = await fetch(`http://localhost:8000/doctorPaymentData/`)
              const data = await fetchdata.json()
              let key = Object.keys(data[0])
              key.forEach(element => {
                console.log(element);
                    let tddata = document.createElement("td")
                    let span = document.createElement("span")
                    span.innerHTML = data[0][element]
                    tddata.append(span)
                    document.getElementById("rowdata").appendChild(tddata)
              });
          }
          fetchPaymentData( )
    </script>
</body>

<!-- <script src="/javascript/doctorPanel/urlPath.js"></script> -->
<script src="/javascript/doctorPanel/sidebar.js"></script>
<script src="/javascript/doctorPanel/viewPayment.js"></script>

</html>