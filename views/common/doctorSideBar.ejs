    const fetchSideBarDetails = async()=>{
      let fetchData = await fetch(`http://localhost:8000/doctorSideBarDetail/4`)
      let data = await fetchData.json()
      console.log(data);
      let key = Object.keys(data[0])
      key.forEach(element => {
        console.log(key);
        document.getElementById(element).innerHTML = data[0][element]
      });
    }
    fetchSideBarDetails()
</script>

